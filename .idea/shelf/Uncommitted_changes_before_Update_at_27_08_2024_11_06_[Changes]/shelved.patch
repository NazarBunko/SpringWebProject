;\r\n            top: 10px;\r\n            right: 20px;\r\n            width: 50px;\r\n            height: 50px;\r\n            background-color: #007bff;\r\n            color: white;\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            border-radius: 4px;\r\n            text-decoration: none;\r\n            font-size: 28px;\r\n            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);\r\n            transform: translateY(-50%);\r\n        }\r\n\r\n        .edit-button {\r\n            background: none;\r\n            border: none;\r\n            cursor: pointer;\r\n            color: #007bff;\r\n            font-size: 24px;\r\n        }\r\n\r\n        .edit-button:hover {\r\n            color: #0056b3;\r\n        }\r\n\r\n        .delete-button {\r\n            background: none;\r\n            border: none;\r\n            cursor: pointer;\r\n            color: #d9534f;\r\n            font-size: 24px;\r\n        }\r\n\r\n        .delete-button:hover {\r\n            color: #c9302c;\r\n        }\r\n\r\n        .no-people-message {\r\n            font-size: 18px;\r\n            color: #555;\r\n        }\r\n    </style>\r\n    <!-- Font Awesome CSS -->\r\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css\">\r\n    <!-- SortableJS -->\r\n    <script src=\"https://cdn.jsdelivr.net/npm/sortablejs@1.14.0/Sortable.min.js\"></script>\r\n</head>\r\n<body>\r\n\r\n<a href=\"/people/new\" class=\"add-button\">+</a>\r\n\r\n<div class=\"scrollable-container\">\r\n    <div class=\"list-wrapper\" id=\"sortable-list\">\r\n        <div th:each=\"person : ${people}\" class=\"person-container\">\r\n            <div class=\"person-photo\" th:style=\"'background-image: url(' + ${person.getPhoto()} + ')'\"></div>\r\n            <div class=\"person-details\">\r\n                <p><strong>Name:</strong> <span th:text=\"${person.decodeHtmlEntities(person.getName())}\">John Doe</span></p>\r\n                <p><strong>Surname:</strong> <span th:text=\"${person.decodeHtmlEntities(person.getSurname())}\">Doe</span></p>\r\n                <p><strong>Email:</strong> <span th:text=\"${person.decodeHtmlEntities(person.getEmail())}\">john.doe@example.com</span></p>\r\n                <!-- Add more details as needed -->\r\n                <a th:href=\"@{/people/person(id=${person.getId()})}\">Details</a>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<script>\r\n    document.addEventListener('DOMContentLoaded', function () {\r\n        var sortableList = document.getElementById('sortable-list');\r\n        Sortable.create(sortableList, {\r\n            animation: 150,\r\n            ghostClass: 'sortable-ghost'\r\n        });\r\n    });\r\n</script>\r\n\r\n</body>\r\n</html>\r\n
